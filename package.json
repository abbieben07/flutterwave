{
    "name": "@nativescript-community/plugin-seed",
    "version": "1.0.0",
    "scripts": {
        "setup": "ts-patch install",
        "build.angular": "ng-packagr -p src/angular/package.json -c src/angular/tsconfig.json",
        "clean": "rimraf ./plugin/**/*.d.ts ./plugin/**/*.js ./plugin/**/*.js.map plugin/node_modules plugin/package-lock.json node_modules",
        "tsc": "cpy '**/*.d.ts' '../plugin' --parents --cwd=src && tsc -skipLibCheck -d",
        "build": "rm -f .tsbuildinfo && npm run tsc ",
        "publish": "npm run setup && npm run build && npm run build.angular && lerna publish --create-release=github",
        "commitmsg": "commitlint -e $GIT_PARAMS",
        "e2e.build": "./e2e/run-tests.js -b",
        "e2e.run": "./e2e/run-tests.js -r",
        "e2e": "./e2e/run-tests.js",
        "doc": "typedoc --out docs --tsconfig tsconfig.doc.json --includeDeclarations --excludeExternals --mode file --ignoreCompilerErrors --excludeNotExported --excludePrivate --theme minimal",
        "lint.demos": "cd demo-ng && npm run lint && cd ../demo-react && npm run lint && cd ../demo-svelte && npm run lint && cd ../demo-vue && npm run lint",
        "format": "npx prettier --write './**/*.{ts,js,md,html,css,scss,json}'",
        "lint": "npm run lint.demos && npm run format && npx eslint . --ext .ts --fix"
    },
    "devDependencies": {
        "@angular/animations": "~10.1.6",
        "@angular/common": "~10.1.6",
        "@angular/compiler": "~10.1.6",
        "@angular/compiler-cli": "~10.1.6",
        "@angular/core": "~10.1.6",
        "@angular/forms": "~10.1.6",
        "@angular/platform-browser": "~10.1.6",
        "@angular/platform-browser-dynamic": "~10.1.6",
        "@angular/router": "~10.1.6",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@nativescript/angular": "10.1.7",
        "@nativescript/core": "7.0.13",
        "@nativescript/ios": "7.0.5",
        "@nativescript/types-android": "~7.0.4",
        "@nativescript/types-ios": "~7.0.4",
        "@nativescript/webpack": "3.0.8",
        "@types/node": "14.14.7",
        "@types/react": "16.9.56",
        "@typescript-eslint/eslint-plugin": "4.7.0",
        "@typescript-eslint/parser": "4.7.0",
        "cpy-cli": "^3.1.1",
        "detox": "^17.14.1",
        "eslint": "7.13.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "jest-circus": "^26.6.3",
        "jest-cli": "^26.6.3",
        "lerna": "^3.22.1",
        "ng-packagr": "~10.1.2",
        "prettier": "^2.1.2",
        "prompt": "^1.0.0",
        "react-nativescript": "^2.1.0",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "svelte-native": "0.9.5",
        "ts-patch": "^1.3.0",
        "tslib": "2.0.3",
        "typedoc": "^0.19.2",
        "typescript": "3.9.7",
        "vue": "^2.6.12",
        "zone.js": "0.11.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nativescript-community/plugin-seed.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/nativescript-community/plugin-seed/issues"
    },
    "homepage": "https://github.com/nativescript-community/plugin-seed#readme",
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "dependencies": {
        "ts-node": "^8.10.2"
    }
}
